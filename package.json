{
    "private": true,
    "scripts": {
        "dev": "cp dev.env .env && next dev ",
        "build": "cp prod.env .env && next build && next export",
        "start": "next start",
        "dBuild": "docker-compose -f compose.dev.yaml up --build",
        "dUp": "cp dev.env .env && docker-compose -f compose.dev.yaml up",
        "pBuild": "cp prod.env .env && sudo rm -rf .next && docker-compose  -f compose.build.yaml  up --build",
        "pUp": "cp prod.env .env && docker-compose -f compose.prod.yaml up",
        "lint": "next lint",
        "info": "next info",
        "pushBuild": "docker ps | grep blog_blogserver | awk {'print $1'} | xargs -i docker commit {} blog &&  docker tag blog ghcr.io/glennstreetman/blog:latest && docker push ghcr.io/glennstreetman/blog:latest",
        "pushBuildOld": "sudo rm -rf .next && ssh ec2 sudo rm -rf /projects/Blog/.next && dotenv -e prod.env next build && scp -r /home/glenn/projects/Blog/.next ec2:/projects/Blog/.next && docker context use prod-context && docker ps -a | grep blog | awk {'print $1'} | xargs -I {} docker stop {} && ssh launchBlog",
        "restartProd": "docker context use prod-context && docker ps -a | grep blog | awk {'print $1'} | xargs -I {} docker stop {} && ssh launchBlog",
        "pullBuild": "docker pull ghcr.io/glennstreetman/blog:latest",
        "samBuild": "sam build",
        "samDeploy": "sam deploy --guided",
        "samUpdate": "sam build && sam deploy",
        "samSync": "aws s3 sync ./out/ s3://blog-cloudformation-staticsitebucket-16zv9z6dktdqq"
    },
